   "source": [
    "# image stadardization\n",
    "image_stnd = np.empty([images.shape[0],512,512])\n",
    "\n",
    "for i in range(0,images.shape[0]):\n",
    "    img_mean = np.mean(images[i])\n",
    "    img_std = np.std(images[i])\n",
    "    image_stnd[i] = (images[i] - img_mean)/img_std"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "3f8ba541",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(256, 512, 512, 3)\n",
      "(256, 512, 512, 2)\n"
     ]
    }
   ],
   "source": [
    "# setup trainX and trainY\n",
    "trainX = np.empty([image_stnd.shape[0],512,512,3])\n",
    "trainX[:,:,:,0] = image_stnd\n",
    "trainX[:,:,:,1] = image_stnd\n",
    "trainX[:,:,:,2] = image_stnd\n",
    "\n",
    "trainY = labels\n",
    "\n",
    "print(trainX.shape)\n",
    "print(trainY.shape)"
   ]
  },
